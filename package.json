{
	"name": "view-package-on-npm",
	"displayName": "NPM Imported Package Links",
	"description": "A Visual Studio extension that displays links to NPM, Git Repo (GitHub/GitLab, etc.) and Homepage for an imported package in javascript and typescript files on hover",
	"version": "0.8.0",
	"publisher": "MainaWycliffe",
	"engines": {
		"vscode": "^1.69.0"
	},
	"repository": {
		"url": "https://github.com/mainawycliffe/view-package-on-npm"
	},
	"icon": "images/npm-imported-packages-lens.jpg",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:typescriptreact",
		"onLanguage:javascriptreact",
		"onLanguage:json"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"breakpoints": [
			{
				"language": "javascript"
			},
			{
				"language": "typescript"
			},
			{
				"language": "javascriptreact"
			},
			{
				"language": "typescriptreact"
			}
		]
	},
	"scripts": {
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "npm run esbuild-base -- --minify",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.69.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"@vscode/test-electron": "^2.1.5",
		"esbuild": "^0.14.49",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"esquery": "^1.4.0",
		"glob": "^8.0.3",
		"prettier": "^2.7.1",
		"ts-loader": "^9.3.1",
		"typescript": "^4.7.4",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
		"@phenomnomnominal/tsquery": "^5.0.0",
		"got": "^12.1.0"
	}
}