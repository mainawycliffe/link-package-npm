{
  "name": "view-package-on-npm",
  "displayName": "NPM Imported Package Links",
  "description": "A Visual Studio extension that displays links to NPM, Git Repo (GitHub/GitLab, etc.) and Homepage for an imported package in javascript and typescript files on hover",
  "version": "0.15.1",
  "publisher": "MainaWycliffe",
  "engines": {
    "vscode": "^1.78.0"
  },
  "repository": {
    "url": "https://github.com/mainawycliffe/view-package-on-npm"
  },
  "icon": "images/npm-imported-packages-lens.jpg",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact",
    "onLanguage:json",
    "onLanguage:vue",
    "onLanguage:astro",
    "onLanguage:svelte"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "breakpoints": [
      {
        "language": "javascript"
      },
      {
        "language": "typescript"
      },
      {
        "language": "javascriptreact"
      },
      {
        "language": "typescriptreact"
      },
      {
        "language": "json"
      },
      {
        "language": "vue"
      },
      {
        "language": "astro"
      },
      {
        "language": "svelte"
      }
    ]
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "release": "release-it"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.8",
    "@types/node": "~16.18.31",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vscode/test-electron": "^2.3.2",
    "babel-jest": "^28.1.3",
    "esbuild": "^0.14.54",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "esquery": "^1.5.0",
    "glob": "^8.1.0",
    "jest": "^28.1.3",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "release-it": "^15.10.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.83.1",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@phenomnomnominal/tsquery": "^5.0.1",
    "got": "^12.6.0",
    "zod": "^3.21.4"
  },
  "packageManager": "yarn@4.0.2"
}
